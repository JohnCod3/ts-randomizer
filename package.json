{
  "name": "@vposd/ts-autofixture",
  "version": "1.0.0",
  "description": "Unit test helper for create random objects by class or interface",
  "main": "main.ts",
  "scripts": {
    "build": "tsc",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "install": "npm run build",
    "test": "tsc && node tests/index.js && npx jest",
    "test-watch": "nodemon --watch src --watch tests -e ts --exec npm run test"
  },
  "author": "Valerii Pozdniakov",
  "license": "ISC",
  "files": [
    "index.js",
    "index.ts",
    "transformer.js",
    "transformer.ts"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/lodash": "^4.14.144",
    "@types/uuid": "^3.4.6",
    "nodemon": "^2.0.1",
    "ts-jest": "^24.2.0",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "path": "^0.12.7",
    "typescript": "^3.6.4"
  }
}
