{
  "name": "ts-randomizer",
  "version": "1.0.1",
  "description": "A tool to create random data by requested types",
  "main": "index.ts",
  "author": {
    "name": "Valerii Posdnyakov",
    "email": "vpposd@gmail.com"
  },
  "homepage": "https://github.com/vposd/ts-randomizer#readme",
  "bugs": {
    "url": "https://github.com/vposd/ts-randomizer/issues"
  },
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "posttest": "npm run check",
    "postversion": "git push && git push --tags",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "report-coverage": "codecov",
    "test": "node tests/index.js && npx jest --coverage",
    "test-watch": "nodemon --watch src --watch tests -e ts --exec npm run test",
    "version": "npm run format && git add -A src"
  },
  "license": "ISC",
  "files": [
    "build/src",
    "build/src/randomizer",
    "build/src/transformer",
    "build/src/types.d.ts",
    "build/src/types.js"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.0.3",
    "@types/uuid": "^3.4.6",
    "codecov": "^3.6.1",
    "gts": "^1.1.2",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "uuid": "^3.3.3",
    "lodash": "^4.17.15"
  }
