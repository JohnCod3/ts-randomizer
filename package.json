{
  "name": "ts-randomizer",
  "version": "1.0.25",
  "description": "A tool to create random data by requested types",
  "main": "index.js",
  "types": "index.d.ts",
  "author": {
    "name": "Valerii Posdnyakov",
    "email": "vpposd@gmail.com"
  },
  "homepage": "https://github.com/vposd/ts-randomizer#readme",
  "bugs": {
    "url": "https://github.com/vposd/ts-randomizer/issues"
  },
  "keywords": [
    "typescript",
    "autofixture",
    "fixture",
    "randomizer",
    "tdd",
    "mock",
    "auto"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "posttest": "npm run check",
    "postversion": "git push && git push --tags",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "report-coverage": "codecov",
    "test": "node test/index.js && npx jest --coverage",
    "test-watch": "nodemon --watch src --watch test -e ts --exec npm run test",
    "version": "npm run check && git add -A src"
  },
  "license": "MIT",
  "files": [
    "index.d.ts",
    "index.js",
    "src/**/*.d.ts",
    "src/**/*.js",
    "transformer.d.ts",
    "transformer.js"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^13.11.1",
    "@types/uuid": "^8.0.0",
    "codecov": "^3.7.0",
    "eslint": "^7.2.0",
    "gts": "^2.0.2",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "uuid": "^8.1.0",
    "lodash": "^4.17.15"
  },
  "peerDependencies": {
    "typescript": "^3.9.5"
  }
}
